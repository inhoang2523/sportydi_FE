{
  "expo": {
    "name": "SPORTIDY",
    "slug": "SPORTIDY",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.sportidy.play",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "20ff2221-4189-4ccf-8204-c871fe041897"
      }
    },
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false,
        "exclude": [
          "react-native-deck-swiper",
          "base-64",
          "date-fns",
          "expo-select-dropdown"
        ]
      }
    }
  },
  "name": "sportidy",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.3",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/datetimepicker": "^8.2.0",
    "@react-navigation/drawer": "^7.0.0",
    "@react-navigation/native-stack": "^7.0.0",
    "axios": "^1.7.7",
    "base-64": "^1.0.0",
    "date-fns": "^4.1.0",
    "expo": "~52.0.7",
    "expo-font": "~13.0.1",
    "expo-image-picker": "~16.0.2",
    "expo-linear-gradient": "~14.0.1",
    "expo-linking": "~7.0.2",
    "expo-router": "~4.0.6",
    "expo-select-dropdown": "^1.1.3",
    "expo-splash-screen": "~0.29.11",
    "expo-status-bar": "~2.0.0",
    "expo-system-ui": "~4.0.3",
    "expo-web-browser": "~14.0.1",
    "metro": "0.81.0",
    "metro-config": "0.81.0",
    "metro-resolver": "0.81.0",
    "react": "18.3.1",
    "react-dom": "^18.3.1",
    "react-native": "0.76.2",
    "react-native-config": "^1.5.3",
    "react-native-confirmation-code-field": "^7.4.0",
    "react-native-deck-swiper": "^2.0.17",
    "react-native-dropdown-picker": "^5.4.6",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-paper": "^5.12.5",
    "react-native-qrcode-svg": "^6.3.2",
    "react-native-ratings": "^8.1.0",
    "react-native-reanimated": "~3.16.1",
    "react-native-screens": "^4.0.0",
    "react-native-vector-icons": "^10.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@types/base-64": "^1.0.2",
    "@types/jest": "^29.5.12",
    "@types/jwt-decode": "^2.2.1",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "^18.0.7",
    "jest": "^29.4.0",
    "jest-expo": "^52.0.2",
    "react-test-renderer": "^18.3.1",
    "typescript": "~5.3.3"
  },
  "private": true,
  "expo.doctor.reactNativeDirectoryCheck.listUnknownPackages": false,
  "overrides": {
    "metro": "0.81.0",
    "metro-resolver": "0.81.0",
    "metro-config": "0.81.0",
    "metro-runtime": "0.81.0",
    "metro-react-native-babel-preset": "0.77.0"
  },
  "experiments": {
    "typedRoutes": false
  }
}
